    {
        "id": "1feb8537a9c2bf90",
        "type": "function",
        "z": "6acbb86292f9f77e",
        "name": "function 5",
        "func": "    let newMsg = {};\n    let dataPointsConnector = flow.get(\"DatapointsConnectorMeta\");\n    let dataPointsUnified = flow.get(\"DatapointsUnifiedMeta\");\n    let dt = new Date();\n\n    let readTopic = flow.get(\"DatapointTopicUoE\").split(\"/\");\n\n    readTopic[7] = \"w\";\n    let writeTopic = readTopic.join(\"/\");\n    newMsg.topic = writeTopic;\n\n    //let numberOfDatapoints = msg.payload[\"vals\"].length;\n    let sum = 0;\n    let foundDatapoint;\n    let tagName\n    let convertTagName;\n    let dataPointUoE;\n    let dataPointUoEID;\n    let newVals = [];\n\n    msg.payload[\"vals\"].forEach(function (element) {\n\n        foundDatapoint = dataPointsConnector.filter(datapoint => datapoint.id === element.id)\n        if (foundDatapoint.length > 0) {\n            tagName = foundDatapoint[0].name;\n            convertTagName = tagName.replace(/\\./g, '_');\n\n            convertTagName = convertTagName.split('_').slice(4).join('_');\n\n            dataPointUoE = dataPointsUnified.filter(datapoint => datapoint.name === convertTagName);\n            if (dataPointUoE.length > 0) {\n                dataPointUoEID = dataPointUoE[0].id;\n\n                newVals.push(\n                    {\n                        \"id\": dataPointUoEID,\n                        \"qc\": 3,\n                        \"ts\": dt.toISOString(),\n                        \"val\": element.val\n                    }\n                )\n            }\n        }\n    });\n\n    newMsg.payload = {\n        \"seq\": 1,\n        \"vals\": newVals\n    };\n    return newMsg;",
