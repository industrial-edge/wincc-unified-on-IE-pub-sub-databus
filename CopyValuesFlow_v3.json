    {
        "id": "b0b53d611f84a257",
        "type": "function",
        "z": "6acbb86292f9f77e",
        "name": "function 6",
        "func": "let newMsg = {};\nlet dataPointsConnector = flow.get(\"DatapointsConnectorMeta\");\nlet dataPointsUnified = flow.get(\"DatapointsUnifiedMeta\");\nlet dt = new Date();\n\nlet readTopic = flow.get(\"DatapointTopicOPC\").split(\"/\");\n\nreadTopic[7] = \"w\";\nreadTopic.pop();\nlet writeTopic = readTopic.join(\"/\");\nnewMsg.topic = writeTopic;\n\n//let numberOfDatapoints = msg.payload[\"vals\"].length;\nlet sum = 0;\nlet foundDatapoint;\nlet tagName\nlet convertTagName;\nlet dataPointUoE;\nlet dataPointUoEID;\nlet newVals = [];\n\nmsg.payload[\"vals\"].forEach(function (element) {\n    dataPointUoE = dataPointsUnified.filter(datapoint => datapoint.id === element.id);\n\n    if (dataPointUoE.length > 0) {\n        tagName = dataPointUoE[0].name;\n\n        function checkDPName(datapoint) {\n            convertTagName = datapoint.name.replace(/\\./g, '_');\n            convertTagName = convertTagName.split('_').slice(4).join('_');\n\n            if (convertTagName == tagName) {\n                return tagName;\n            }\n        }\n\n        foundDatapoint = dataPointsConnector.filter(checkDPName);\n        if (foundDatapoint.length > 0) {\n            foundDatapoint = foundDatapoint[0].id;\n\n            newVals.push(\n                {\n                    \"id\": foundDatapoint,\n                    \"qc\": 3,\n                    \"ts\": dt.toISOString(),\n                    \"val\": element.val\n                }\n            )\n        }\n    }\n});\n\nnewMsg.payload = {\n    \"seq\": 1,\n    \"vals\": newVals\n};\nreturn newMsg;",
